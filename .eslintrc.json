{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "semi": 1,
    "react/jsx-filename-extension": ["error", { "extensions": [".jsx"] }],
    "indent": ["error", 2],
    "import/prefer-default-export": 0
  }
}
